<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000"
        motion:motionInterpolator="linear">
       <KeyFrameSet>
           <KeyPosition />
       </KeyFrameSet>
        <OnSwipe motion:touchAnchorId="@+id/layout2" />
        <OnClick motion:targetId="@+id/mainbtn" />
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="1.0"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/layout2" />
        <Constraint
            android:id="@+id/mainbtn"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="90dp"
            android:layout_height="40dp"
            motion:layout_constraintBottom_toTopOf="@+id/layout2"
            motion:layout_constraintVertical_bias="1.0"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:rotationX="0" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/editTextNumberDecimal"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/barcodetext"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintVertical_bias="0.0"
            motion:layout_constraintHorizontal_bias="0.393"
            android:layout_marginTop="24dp" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="@+id/editTextNumberDecimal"
            android:layout_width="wrap_content"
            android:id="@+id/textView6"
            motion:layout_constraintTop_toTopOf="@+id/editTextNumberDecimal"
            motion:layout_constraintEnd_toStartOf="@+id/editTextNumberDecimal"
            motion:layout_constraintStart_toStartOf="@+id/textView3" />
        <Constraint
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="@+id/layout2"
            motion:layout_constraintVertical_bias="0.100000024"
            motion:layout_constraintHorizontal_bias="0.393"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/barcodetext" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/editTextNumberDecimal"
            android:id="@+id/radioButton2"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.100000024"
            motion:layout_constraintHorizontal_bias="0.73" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/radioButton"
            motion:layout_constraintEnd_toStartOf="@+id/radioButton2"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="@+id/radioButton2"
            motion:layout_constraintVertical_bias="0.07999998"
            motion:layout_constraintTop_toTopOf="@+id/radioButton2" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="@+id/weightradiobtn"
            android:layout_width="wrap_content"
            android:id="@+id/countradiobtn"
            motion:layout_constraintTop_toTopOf="@+id/weightradiobtn"
            motion:layout_constraintEnd_toStartOf="@+id/weightradiobtn"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/weightradiobtn"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/editTextNumberDecimal"
            motion:layout_constraintBottom_toBottomOf="@+id/layout2" />
        <Constraint
            motion:layout_constraintEnd_toEndOf="@+id/editTextNumberDecimal"
            android:id="@+id/radiogroup"
            motion:layout_constraintStart_toStartOf="@+id/editTextNumberDecimal"
            motion:layout_constraintTop_toBottomOf="@+id/editTextNumberDecimal"
            android:layout_width="208dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.19" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="@+id/layout2"
            android:layout_width="wrap_content"
            android:id="@+id/addprodutcbtn"
            motion:layout_constraintEnd_toEndOf="@+id/radiogroup"
            motion:layout_constraintStart_toStartOf="@+id/radiogroup"
            motion:layout_constraintTop_toBottomOf="@+id/radiogroup"
            motion:layout_constraintVertical_bias="0.153"
            motion:layout_constraintHorizontal_bias="0.554" />
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/addprodutcbtn"
            motion:layout_constraintEnd_toEndOf="@+id/radiogroup"
            motion:layout_constraintStart_toStartOf="@+id/radiogroup"
            motion:layout_constraintTop_toBottomOf="@+id/radiogroup"
            motion:layout_constraintBottom_toBottomOf="@+id/layout2"
            motion:layout_constraintVertical_bias="0.153" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/textView8"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toTopOf="@+id/barcodetext" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/mainbtn"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="90dp"
            android:layout_height="40dp"
            motion:layout_constraintBottom_toTopOf="@+id/layout2"
            motion:layout_constraintVertical_bias="1.0"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:rotationX="170" />
        <Constraint
            android:id="@+id/textView3"
            motion:layout_constraintEnd_toStartOf="@+id/barcodetext"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="@+id/barcodetext"
            motion:layout_constraintHorizontal_bias="0.444"
            motion:layout_constraintTop_toTopOf="@+id/barcodetext"
            motion:layout_constraintStart_toStartOf="parent"
            android:visibility="visible"
            android:alpha="0" />
        <Constraint
            android:id="@+id/barcodetext"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="@+id/layout2"
            motion:layout_constraintVertical_bias="0.100000024"
            motion:layout_constraintHorizontal_bias="0.393"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:visibility="invisible"
            android:alpha="0" />
        <Constraint
            android:id="@+id/searchbtn"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toEndOf="@+id/barcodetext"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="@+id/barcodetext"
            motion:layout_constraintVertical_bias="0.666"
            motion:layout_constraintHorizontal_bias="0.551"
            motion:layout_constraintTop_toTopOf="@+id/barcodetext"
            android:visibility="invisible"
            android:alpha="0" />
        <Constraint
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteY="188dp"
            motion:layout_editor_absoluteX="9dp"
            android:visibility="visible"
            android:alpha="0" />
        <Constraint
            android:id="@+id/radiogroup"
            android:layout_width="208dp"
            android:layout_height="wrap_content"
            android:visibility="invisible"
            motion:layout_constraintEnd_toEndOf="@id/editTextNumberDecimal"
            motion:layout_constraintStart_toStartOf="@id/editTextNumberDecimal"
            motion:layout_constraintTop_toBottomOf="@+id/editTextNumberDecimal"
            motion:layout_constraintVertical_bias="0.19"
            android:layout_marginStart="16dp"
            motion:layout_constraintBottom_toBottomOf="@+id/layout2"
            android:alpha="0" />
        <Constraint
            android:id="@+id/editTextNumberDecimal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="invisible"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/barcodetext"
            motion:layout_constraintBottom_toBottomOf="@+id/layout2"
            motion:layout_constraintVertical_bias="0.0"
            android:layout_marginTop="24dp"
            motion:layout_constraintHorizontal_bias="0.39"
            android:alpha="0"
            motion:transitionEasing="cubic(0.2,0.2,0.59,0.58)" />
        <Constraint
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="match_parent"
            android:layout_height="500dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="1.0"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/layout2" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintEnd_toEndOf="@+id/radiogroup"
            android:layout_width="wrap_content"
            android:id="@+id/addprodutcbtn"
            motion:layout_constraintStart_toStartOf="@+id/radiogroup"
            motion:layout_constraintTop_toBottomOf="@+id/radiogroup"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:alpha="0"
            android:visibility="invisible"
            motion:layout_constraintVertical_bias="0.15" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toTopOf="@+id/barcodetext"
            android:layout_width="wrap_content"
            android:id="@+id/textView8"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:alpha="0"
            android:visibility="invisible" />
    </ConstraintSet>
</MotionScene>